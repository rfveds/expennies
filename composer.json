{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "ext-pdo": "*",
        "doctrine/dbal": "^4.0.4",
        "doctrine/migrations": "^3.7.4",
        "doctrine/orm": "^3.2",
        "php-di/php-di": "^7.0",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.1",
        "slim/psr7": "1.6.1.0",
        "slim/slim": "^4.14",
        "slim/twig-view": "^3.4",
        "symfony/cache": "^6.4",
        "symfony/console": "^6.4",
        "symfony/twig-bridge": "^6.4",
        "symfony/webpack-encore-bundle": "^1.15",
        "twig/intl-extra": "^3.4",
        "twig/twig": "3.10.*",
        "vlucas/phpdotenv": "^5.4",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
